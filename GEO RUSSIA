        "# –£—Å—Ç–∞–Ω–æ–≤–∫–∞ –Ω–µ–æ–±—Ö–æ–¥–∏–º—ã—Ö –±–∏–±–ª–∏–æ—Ç–µ–∫\n",
        "!pip install openpyxl pandas -q\n",
        "\n",
        "import pandas as pd\n",
        "import re\n",
        "from google.colab import files\n",
        "import os\n",
        "\n",
        "def extract_cities_from_rosstat(file_path: str) -> pd.DataFrame:\n",
        "    \"\"\"\n",
        "    –ò–∑–≤–ª–µ–∫–∞–µ—Ç –≥–æ—Ä–æ–¥–∞ –∏–∑ —Ñ–∞–π–ª–∞ –†–æ—Å—Å—Ç–∞—Ç\n",
        "    –ò—â–µ—Ç —Å—Ç—Ä–æ–∫–∏, –Ω–∞—á–∏–Ω–∞—é—â–∏–µ—Å—è —Å \"–≥ \" –∏ –ø—Ä–∏–≤—è–∑—ã–≤–∞–µ—Ç –∏—Ö –∫ –æ–±–ª–∞—Å—Ç—è–º\n",
        "    \"\"\"\n",
        "\n",
        "    # –ó–∞–≥—Ä—É–∂–∞–µ–º —Ñ–∞–π–ª\n",
        "    df = pd.read_excel(file_path)\n",
        "\n",
        "    print(f\"–ó–∞–≥—Ä—É–∂–µ–Ω–æ —Å—Ç—Ä–æ–∫: {len(df)}\")\n",
        "    print(f\"–ö–æ–ª–æ–Ω–∫–∏: {list(df.columns)}\")\n",
        "    print(\"\\n–ü–µ—Ä–≤—ã–µ 10 —Å—Ç—Ä–æ–∫:\")\n",
        "    print(df.head(10))\n",
        "\n",
        "    # –û–ø—Ä–µ–¥–µ–ª—è–µ–º –∫–æ–ª–æ–Ω–∫–∏\n",
        "    city_column = df.columns[0]\n",
        "    population_column = df.columns[1]\n",
        "\n",
        "    results = []\n",
        "    current_region = \"\"\n",
        "\n",
        "    for idx, row in df.iterrows():\n",
        "        city_name = str(row[city_column]).strip()\n",
        "        population = row[population_column]\n",
        "\n",
        "        # –ü—Ä–æ–ø—É—Å–∫–∞–µ–º –ø—É—Å—Ç—ã–µ —Å—Ç—Ä–æ–∫–∏\n",
        "        if pd.isna(row[city_column]) or city_name == '' or city_name == 'nan':\n",
        "            continue\n",
        "\n",
        "        # –û–ø—Ä–µ–¥–µ–ª—è–µ–º —Ä–µ–≥–∏–æ–Ω (–†–µ—Å–ø—É–±–ª–∏–∫–∞, –æ–±–ª–∞—Å—Ç—å, –∫—Ä–∞–π –∏ —Ç.–¥.)\n",
        "        is_region = False\n",
        "\n",
        "        # –ü—Ä–æ–≤–µ—Ä—è–µ–º –∫–ª—é—á–µ–≤—ã–µ —Å–ª–æ–≤–∞ –¥–ª—è —Ä–µ–≥–∏–æ–Ω–æ–≤\n",
        "        region_keywords = [\n",
        "            '–†–µ—Å–ø—É–±–ª–∏–∫–∞',\n",
        "            '–æ–±–ª–∞—Å—Ç—å',\n",
        "            '–∫—Ä–∞–π',\n",
        "            '–∞–≤—Ç–æ–Ω–æ–º–Ω—ã–π –æ–∫—Ä—É–≥',\n",
        "            '–∞–≤—Ç–æ–Ω–æ–º–Ω–∞—è –æ–±–ª–∞—Å—Ç—å'\n",
        "        ]\n",
        "\n",
        "        # –ü—Ä–æ–≤–µ—Ä—è–µ–º –Ω–∞–ª–∏—á–∏–µ –∫–ª—é—á–µ–≤—ã—Ö —Å–ª–æ–≤\n",
        "        for keyword in region_keywords:\n",
        "            if keyword in city_name:\n",
        "                is_region = True\n",
        "                break\n",
        "\n",
        "        # –û—Ç–¥–µ–ª—å–Ω–∞—è –ø—Ä–æ–≤–µ—Ä–∫–∞ –¥–ª—è –ú–æ—Å–∫–≤—ã –∏ –°–∞–Ω–∫—Ç-–ü–µ—Ç–µ—Ä–±—É—Ä–≥–∞\n",
        "        if city_name == '–ú–æ—Å–∫–≤–∞' or city_name == '–°–∞–Ω–∫—Ç-–ü–µ—Ç–µ—Ä–±—É—Ä–≥' or city_name == '–°–µ–≤–∞—Å—Ç–æ–ø–æ–ª—å':\n",
        "            is_region = True\n",
        "\n",
        "        if is_region:\n",
        "            current_region = city_name\n",
        "            print(f\"–ù–∞–π–¥–µ–Ω —Ä–µ–≥–∏–æ–Ω: {current_region}\")\n",
        "            continue\n",
        "\n",
        "        # –ò—â–µ–º –≥–æ—Ä–æ–¥–∞, –Ω–∞—á–∏–Ω–∞—é—â–∏–µ—Å—è —Å \"–≥ \"\n",
        "        if city_name.startswith('–≥ '):\n",
        "            # –ò–∑–≤–ª–µ–∫–∞–µ–º –Ω–∞–∑–≤–∞–Ω–∏–µ –≥–æ—Ä–æ–¥–∞ –±–µ–∑ –ø—Ä–µ—Ñ–∏–∫—Å–∞ \"–≥ \"\n",
        "            city_clean = city_name[2:].strip()\n",
        "\n",
        "            # –ü—Ä–æ–≤–µ—Ä—è–µ–º, —á—Ç–æ –Ω–∞—Å–µ–ª–µ–Ω–∏–µ - —ç—Ç–æ —á–∏—Å–ª–æ\n",
        "            try:\n",
        "                pop_value = pd.to_numeric(population, errors='coerce')\n",
        "\n",
        "                if not pd.isna(pop_value) and pop_value > 0:\n",
        "                    results.append({\n",
        "                        '–ì–û–†–û–î': city_clean,\n",
        "                        '–û–ë–õ–ê–°–¢–¨': current_region,\n",
        "                        '–ù–ê–°–ï–õ–ï–ù–ò–ï': int(pop_value)\n",
        "                    })\n",
        "\n",
        "                    # –í—ã–≤–æ–¥–∏–º –¥–ª—è –æ—Ç–ª–∞–¥–∫–∏ –ø–µ—Ä–≤—ã–µ 10\n",
        "                    if len(results) <= 10:\n",
        "                        print(f\"–î–æ–±–∞–≤–ª–µ–Ω –≥–æ—Ä–æ–¥: {city_clean} | {current_region} | {int(pop_value)}\")\n",
        "            except Exception as e:\n",
        "                print(f\"–û—à–∏–±–∫–∞ –æ–±—Ä–∞–±–æ—Ç–∫–∏: {city_name} - {e}\")\n",
        "                continue\n",
        "\n",
        "    return pd.DataFrame(results)\n",
        "\n",
        "def main():\n",
        "    print(\"=\" * 80)\n",
        "    print(\"–ò–ó–í–õ–ï–ß–ï–ù–ò–ï –ì–û–†–û–î–û–í –ò–ó –§–ê–ô–õ–ê –†–û–°–°–¢–ê–¢\")\n",
        "    print(\"=\" * 80)\n",
        "\n",
        "    # –ü—Ä–æ–≤–µ—Ä—è–µ–º —Ñ–∞–π–ª—ã –≤ —Ç–µ–∫—É—â–µ–π –¥–∏—Ä–µ–∫—Ç–æ—Ä–∏–∏\n",
        "    print(\"\\nüìÅ –§–∞–π–ª—ã –≤ —Ç–µ–∫—É—â–µ–π –¥–∏—Ä–µ–∫—Ç–æ—Ä–∏–∏:\")\n",
        "    current_files = os.listdir('.')\n",
        "    for f in current_files:\n",
        "        if 'rosstat' in f.lower() or 'cities' in f.lower():\n",
        "            print(f\"  ‚úì {f}\")\n",
        "\n",
        "    # –ò—â–µ–º —Ñ–∞–π–ª —Å —Ä–∞–∑–Ω—ã–º–∏ —Ä–∞—Å—à–∏—Ä–µ–Ω–∏—è–º–∏\n",
        "    possible_names = [\n",
        "        'all cities rosstat with populations.xlsx',\n",
        "        'all cities rosstat with populations.xls',\n",
        "        'all cities rosstat with populations.csv',\n",
        "        'all cities rosstat with populations',\n",
        "    ]\n",
        "\n",
        "    file_path = None\n",
        "    for name in possible_names:\n",
        "        if os.path.exists(name):\n",
        "            file_path = name\n",
        "            break\n",
        "        # –ü—Ä–æ–≤–µ—Ä—è–µ–º —Å —Ä–∞—Å—à–∏—Ä–µ–Ω–∏—è–º–∏\n",
        "        for ext in ['.xlsx', '.xls', '.csv']:\n",
        "            full_name = name + ext if not name.endswith(ext) else name\n",
        "            if os.path.exists(full_name):\n",
        "                file_path = full_name\n",
        "                break\n",
        "        if file_path:\n",
        "            break\n",
        "\n",
        "    if file_path is None:\n",
        "        print(f\"\\n‚ùå –§–∞–π–ª –Ω–µ –Ω–∞–π–¥–µ–Ω –∞–≤—Ç–æ–º–∞—Ç–∏—á–µ—Å–∫–∏!\")\n",
        "        print(\"\\nüì§ –ó–∞–≥—Ä—É–∑–∏—Ç–µ —Ñ–∞–π–ª –≤—Ä—É—á–Ω—É—é:\")\n",
        "        uploaded = files.upload()\n",
        "        file_path = list(uploaded.keys())[0]\n",
        "        print(f\"\\n‚úÖ –§–∞–π–ª –∑–∞–≥—Ä—É–∂–µ–Ω: {file_path}\")\n",
        "    else:\n",
        "        print(f\"\\n‚úÖ –ù–∞–π–¥–µ–Ω —Ñ–∞–π–ª: {file_path}\")\n",
        "\n",
        "    print(f\"\\nüìÇ –û–±—Ä–∞–±–æ—Ç–∫–∞ —Ñ–∞–π–ª–∞: {file_path}\")\n",
        "    print(\"-\" * 80)\n",
        "\n",
        "    try:\n",
        "        result_df = extract_cities_from_rosstat(file_path)\n",
        "\n",
        "        print(\"\\n\" + \"=\" * 80)\n",
        "        print(f\"‚úÖ –û–ë–†–ê–ë–û–¢–ö–ê –ó–ê–í–ï–†–®–ï–ù–ê\")\n",
        "        print(\"=\" * 80)\n",
        "        print(f\"–í—Å–µ–≥–æ –Ω–∞–π–¥–µ–Ω–æ –≥–æ—Ä–æ–¥–æ–≤: {len(result_df)}\")\n",
        "\n",
        "        if len(result_df) == 0:\n",
        "            print(\"\\n‚ö†Ô∏è –ì–æ—Ä–æ–¥–∞ –Ω–µ –Ω–∞–π–¥–µ–Ω—ã! –ü—Ä–æ–≤–µ—Ä—å—Ç–µ —Å—Ç—Ä—É–∫—Ç—É—Ä—É —Ñ–∞–π–ª–∞.\")\n",
        "            return None\n",
        "\n",
        "        # –°—Ç–∞—Ç–∏—Å—Ç–∏–∫–∞ –ø–æ —Ä–µ–≥–∏–æ–Ω–∞–º\n",
        "        print(\"\\nüìä –°—Ç–∞—Ç–∏—Å—Ç–∏–∫–∞ –ø–æ —Ä–µ–≥–∏–æ–Ω–∞–º:\")\n",
        "        print(\"-\" * 80)\n",
        "        region_counts = result_df['–û–ë–õ–ê–°–¢–¨'].value_counts()\n",
        "        for region, count in region_counts.head(10).items():\n",
        "            print(f\"  {region}: {count} –≥–æ—Ä–æ–¥–æ–≤\")\n",
        "\n",
        "        # –°—Ç–∞—Ç–∏—Å—Ç–∏–∫–∞ –ø–æ –Ω–∞—Å–µ–ª–µ–Ω–∏—é\n",
        "        print(\"\\nüìä –°—Ç–∞—Ç–∏—Å—Ç–∏–∫–∞ –ø–æ –Ω–∞—Å–µ–ª–µ–Ω–∏—é:\")\n",
        "        print(\"-\" * 80)\n",
        "        print(f\"  –ú–∏–Ω–∏–º–∞–ª—å–Ω–æ–µ: {result_df['–ù–ê–°–ï–õ–ï–ù–ò–ï'].min():,}\")\n",
        "        print(f\"  –ú–∞–∫—Å–∏–º–∞–ª—å–Ω–æ–µ: {result_df['–ù–ê–°–ï–õ–ï–ù–ò–ï'].max():,}\")\n",
        "        print(f\"  –°—Ä–µ–¥–Ω–µ–µ: {result_df['–ù–ê–°–ï–õ–ï–ù–ò–ï'].mean():,.0f}\")\n",
        "        print(f\"  –û–±—â–µ–µ –Ω–∞—Å–µ–ª–µ–Ω–∏–µ –≤—Å–µ—Ö –≥–æ—Ä–æ–¥–æ–≤: {result_df['–ù–ê–°–ï–õ–ï–ù–ò–ï'].sum():,}\")\n",
        "\n",
        "        # –¢–æ–ø-20 –≥–æ—Ä–æ–¥–æ–≤ –ø–æ –Ω–∞—Å–µ–ª–µ–Ω–∏—é\n",
        "        print(\"\\nüèÜ –¢–û–ü-20 –ì–û–†–û–î–û–í –ü–û –ù–ê–°–ï–õ–ï–ù–ò–Æ:\")\n",
        "        print(\"-\" * 80)\n",
        "        top_cities = result_df.nlargest(20, '–ù–ê–°–ï–õ–ï–ù–ò–ï')\n",
        "        for idx, row in top_cities.iterrows():\n",
        "            print(f\"  {row['–ì–û–†–û–î']:30} | {row['–û–ë–õ–ê–°–¢–¨']:35} | {row['–ù–ê–°–ï–õ–ï–ù–ò–ï']:>10,}\")\n",
        "\n",
        "        # –ü–æ–∫–∞–∑—ã–≤–∞–µ–º –ø–µ—Ä–≤—ã–µ 30 –∑–∞–ø–∏—Å–µ–π\n",
        "        print(\"\\n\" + \"=\" * 80)\n",
        "        print(\"–ü–ï–†–í–´–ï 30 –ó–ê–ü–ò–°–ï–ô:\")\n",
        "        print(\"=\" * 80)\n",
        "        print(result_df.head(30).to_string(index=False))\n",
        "\n",
        "        # –°–æ—Ö—Ä–∞–Ω–µ–Ω–∏–µ —Ä–µ–∑—É–ª—å—Ç–∞—Ç–∞\n",
        "        print(\"\\nüíæ –°–æ—Ö—Ä–∞–Ω–µ–Ω–∏–µ —Ä–µ–∑—É–ª—å—Ç–∞—Ç–∞...\")\n",
        "        output_file = \"cities_with_population_clean.xlsx\"\n",
        "        result_df.to_excel(output_file, index=False, engine='openpyxl')\n",
        "        print(f\"‚úÖ –†–µ–∑—É–ª—å—Ç–∞—Ç —Å–æ—Ö—Ä–∞–Ω–µ–Ω –≤ —Ñ–∞–π–ª: {output_file}\")\n",
        "\n",
        "        # –°–∫–∞—á–∏–≤–∞–Ω–∏–µ —Ñ–∞–π–ª–∞\n",
        "        print(\"\\nüì• –°–∫–∞—á–∏–≤–∞–Ω–∏–µ —Ä–µ–∑—É–ª—å—Ç–∞—Ç–∞...\")\n",
        "        files.download(output_file)\n",
        "\n",
        "        return result_df\n",
        "\n",
        "    except Exception as e:\n",
        "        print(f\"\\n‚ùå –û—à–∏–±–∫–∞: {e}\")\n",
        "        import traceback\n",
        "        traceback.print_exc()\n",
        "        return None\n",
        "\n",
        "# –ó–ê–ü–£–°–ö –°–ö–†–ò–ü–¢–ê\n",
        "result = main()\n",
        "\n",
        "# –î–æ–ø–æ–ª–Ω–∏—Ç–µ–ª—å–Ω—ã–π –∞–Ω–∞–ª–∏–∑ (–µ—Å–ª–∏ –Ω—É–∂–Ω–æ)\n",
        "if result is not None and len(result) > 0:\n",
        "    print(\"\\n\" + \"=\" * 80)\n",
        "    print(\"–î–û–ü–û–õ–ù–ò–¢–ï–õ–¨–ù–ê–Ø –ò–ù–§–û–†–ú–ê–¶–ò–Ø\")\n",
        "    print(\"=\" * 80)\n",
        "\n",
        "    # –ì–æ—Ä–æ–¥–∞ —Å –Ω–∞—Å–µ–ª–µ–Ω–∏–µ–º –±–æ–ª–µ–µ 1 –º–ª–Ω\n",
        "    big_cities = result[result['–ù–ê–°–ï–õ–ï–ù–ò–ï'] > 1000000]\n",
        "    print(f\"\\nüèôÔ∏è –ì–æ—Ä–æ–¥–∞ —Å –Ω–∞—Å–µ–ª–µ–Ω–∏–µ–º –±–æ–ª–µ–µ 1 –º–ª–Ω: {len(big_cities)}\")\n",
        "    if len(big_cities) > 0:\n",
        "        for idx, row in big_cities.iterrows():\n",
        "            print(f\"  {row['–ì–û–†–û–î']}: {row['–ù–ê–°–ï–õ–ï–ù–ò–ï']:,}\")\n",
        "\n",
        "    # –ì–æ—Ä–æ–¥–∞ —Å –Ω–∞—Å–µ–ª–µ–Ω–∏–µ–º –º–µ–Ω–µ–µ 10 —Ç—ã—Å\n",
        "    small_cities = result[result['–ù–ê–°–ï–õ–ï–ù–ò–ï'] < 10000]\n",
        "    print(f\"\\nüèòÔ∏è –ì–æ—Ä–æ–¥–∞ —Å –Ω–∞—Å–µ–ª–µ–Ω–∏–µ–º –º–µ–Ω–µ–µ 10 —Ç—ã—Å: {len(small_cities)}\")\n",
        "\n",
        "    # –†–∞—Å–ø—Ä–µ–¥–µ–ª–µ–Ω–∏–µ –ø–æ —Ä–∞–∑–º–µ—Ä–∞–º\n",
        "    print(\"\\nüìä –†–∞—Å–ø—Ä–µ–¥–µ–ª–µ–Ω–∏–µ –≥–æ—Ä–æ–¥–æ–≤ –ø–æ —Ä–∞–∑–º–µ—Ä—É:\")\n",
        "    print(f\"  –ë–æ–ª–µ–µ 1 –º–ª–Ω: {len(result[result['–ù–ê–°–ï–õ–ï–ù–ò–ï'] > 1000000])}\")\n",
